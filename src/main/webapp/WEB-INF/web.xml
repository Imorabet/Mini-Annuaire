  	<description></description>
  	<servlet-class>com.example.web.controller.rechercheEtudiant</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>getAllDepartements</servlet-name>
  	<url-pattern>/getAllDepartements</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>getAllFilieres</servlet-name>
  	<url-pattern>/getAllFilieres</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>getAllEtudiants</servlet-name>
  	<url-pattern>/getAllEtudiants</url-pattern>
