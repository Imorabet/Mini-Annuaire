  <servlet>
  	<servlet-name>getAllFilieres</servlet-name>
  	<display-name>getAllFilieres</display-name>
  	<description></description>
  	<servlet-class>com.example.web.controller.getAllFilieres</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>getAllEtudiants</servlet-name>
  	<display-name>getAllEtudiants</display-name>
  	<description></description>
  	<servlet-class>com.example.web.controller.getAllEtudiants</servlet-class>
  </servlet>
  <servlet>
